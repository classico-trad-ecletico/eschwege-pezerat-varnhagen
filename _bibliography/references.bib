@phdthesis{zequini:2007arqueologia,
  title = {{Arqueologia de uma fábrica de ferro : morro de Araçoiaba séculos XVI-XVIII}},
  shorttitle = {{Arqueologia de uma fábrica de ferro}},
  author = {Zequini, Anicleide},
  year = {2007},
  month = mar,
  address = {{São Paulo}},
  doi = {10.11606/T.71.2007.tde-25062007-151536},
  abstract = {Esta Tese tem como objetivo analisar a pesquisa arqueológica e os artefatos coletados no Sitio Arqueológico Afonso Sardinha, localizado no morro de Araçoiaba, Iperó - São Paulo. O resultado da análise permitiu concluir que a área corresponde a um campo de mineração de ferro com a presença de evidências de uma Fábrica de Ferro, de um sistema de aproveitamento de energia hidráulica para movimentar os equipamentos e ferramentas destinadas a produção e de fornos de fundição. Além disso, a datação do material cerâmico (vasilhames e telhas) indica que as atividades de exploração do minério tiveram início no século XVI.},
